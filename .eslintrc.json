{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-plusplus": "off",
        "no-loop-func": "off",
        "no-param-reassign": "off",
        "no-mixed-operators": "off",
        "default-case": "off"
    }
}
